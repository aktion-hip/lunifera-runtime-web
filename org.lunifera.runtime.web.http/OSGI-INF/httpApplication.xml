<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="org.lunifera.runtime.web.http.application">
   <implementation class="org.lunifera.runtime.web.http.HttpApplication"/>
   <property name="lunifera.http.id" type="String" value="lunifera.http.application.default"/>
   <property name="lunifera.http.name" type="String" value="defaultapplication"/>
   <property name="lunifera.http.contextPath" type="String" value="/"/>
   <service>
      <provide interface="org.lunifera.runtime.web.http.IHttpApplication"/>
   </service>
   <reference bind="setJetty" cardinality="0..1" interface="org.lunifera.runtime.web.jetty.IJettyService" name="jettyService" policy="dynamic" unbind="unsetJetty"/>
</scr:component>
